	game:GetService("TweenService"):Create(MainGui.Frames.MainButtonsGui.Button1Background.Button1.ImageButton2,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(122, 167, 134)}):Play()
	AimBotting=false
end)
