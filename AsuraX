--key toggle
local userInputService = game:GetService("UserInputService")

local inputMenu = Enum.KeyCode.M
local inputMenuActive = false
local inputAimbot = Enum.KeyCode.R
local inputAimbotActive = false
local inputAimAssist = Enum.KeyCode.T
--local inputMenu = Enum.KeyCode.M
--local inputMenu = Enum.KeyCode.M

userInputService.InputBegan:Connect(function(input)
	if input.KeyCode == inputMenu then
		if MenuHidden == false then
			MenuHidden=true
			MainGui.Visible = true
		else
			MenuHidden=false
			MainGui.Visible = false
		end
	end

	return
end)
userInputService.InputBegan:Connect(function(input)
	if input.KeyCode == inputAimbot then
		if inputAimbotActive == true then
			if AimBotting == true then
				MainGui.Frames.MainButtonsGui.Button1Background.Button1.ImageButton1:TweenPosition(--Timer
					UDim2.new(0, 2,-0.667, 0),
