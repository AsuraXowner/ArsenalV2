			dragging2 = true
			dragStart2 = input.Position
			startPos2 = gui2.Position

			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging2 = false
				end
			end)
		end
	end)

	gui2.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput2 = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput2 and dragging2 then
			update2(input)
		end
	end)



-- Slider3.Script1 
	local Mouse = game.Players.LocalPlayer:GetMouse()
	local Slider = TestGui.MainBackgroundFrame.DownGui.ButtonsListOfSettings.Button1Settings5.Slider1
	local Fill = TestGui.MainBackgroundFrame.DownGui.ButtonsListOfSettings.Button1Settings5.Slider1.Fill2
local Trigger = TestGui.MainBackgroundFrame.DownGui.ButtonsListOfSettings.Button1Settings5.Slider1.TriggerSmoothing
	local TextNumbers = TestGui.MainBackgroundFrame.DownGui.ButtonsListOfSettings.Button1Settings5.Numbers

	local maxvalue = 7 or 700/700
	local startingvalue = 0 or 0/100

	Fill.Size = UDim2.fromScale(Configs.Aimbot.Fov,1)
	TextNumbers.Text = tostring(math.round(Configs.Aimbot.Fov*100))

	local TweenService = game:GetService("TweenService")
	local TweenStyle =  TweenInfo.new(0.25, Enum.EasingStyle.Exponential)

	local function UpdateSlider()
		local OutPut = math.clamp(((Vector2.new(Mouse.X,Mouse.Y)-Slider.AbsolutePosition)/Slider.AbsoluteSize).X,0,1)

		if Configs.Aimbot.Fov ~= OutPut then
			TweenService:Create(Fill,TweenStyle,{Size = UDim2.fromScale(OutPut,1)}):Play()
		end

		Configs.Aimbot.Fov = (startingvalue +(OutPut*(maxvalue-startingvalue)))*100
		TextNumbers.Text = tostring(math.round(Configs.Aimbot.Fov*100))
	end

	Fill:GetPropertyChangedSignal("Size"):Connect(function()
		TextNumbers.Text = tostring(math.round(Configs.Aimbot.Fov*100))
	end)

	local SliderActive = false

	local function ActivateSlider()
		SliderActive = true
		while SliderActive do
			UpdateSlider()
			task.wait(0.01)
		end
	end

	Trigger.MouseButton1Down:Connect(ActivateSlider)

	game:GetService("UserInputService").InputEnded:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			SliderActive = false
		end
	end)
