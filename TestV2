--local MainGui = 
local Button1SettingsValue = false
local Button2SettingsValue = false
local Button3SettingsValue = false
local Button4SettingsValue = false
local Button5SettingsValue = false
local MenuHidden = false
local AimbotFrame = true
local PeakAssistFrame = false
local RenderFrame = false
local SettingsFrame = false
local TracersLineColor = Color3.fromRGB(75,0,10)
local ColorPickerValue = "EspColorText"
local MainGui = TestGui.MainBackgroundFrame
--EspSettings
local EspTextLabelBackground = 1
local EspTextLabelFont = Enum.Font.SourceSansSemibold
local EspTextLabelSize = 20
local EspTextLabelColor3 = Color3.new(1, 1, 1)
local EspTextLabelStrokeTransparency = 0

