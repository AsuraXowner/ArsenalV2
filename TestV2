		else
			if Instances.Version.Value > "1.09" then
				Sound3:play()
				warn("Client using unknown version of AsuraX script cannot load")
				TextNotify="Script cannot load wrong version"
				ImageID="rbxassetid://12988744896"
				NotifyColor=Color3.new(1, 0.32549, 0.337255)
				TestGui.Instances.Messages:Clone().Parent = ListMessage
				ListMessage.Messages.BackgroundColor3 = NotifyColor
				ListMessage.Messages.BackgroundTimer.BackgroundLoad.BackgroundColor3 = NotifyColor
				ListMessage.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageColor3 = NotifyColor
				ListMessage.Messages.BackgroundTimer.Others.Texts.InfoText.TextColor3 = NotifyColor
				ListMessage.Messages.BackgroundTimer.Others.Images.ImageLabel.Image = ImageID
				ListMessage.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, -255,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)
				wait(0.4)
				ListMessage.Messages.BackgroundTimer.Others.Texts.InfoText.Text = TextNotify
				ListMessage.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, 0,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)
				ListMessage.Messages.BackgroundTransparency=0
				ListMessage.Messages.BackgroundTimer.BackgroundTransparency=0
				ListMessage.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=0
				ListMessage.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=0
				ListMessage.Messages:TweenSize(--Timer
					UDim2.new(0, 0,0, 50),
					"Out",
					"Quad",
					NotifyTime,
					false
				)
				wait(NotifyTime)
				ListMessage.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, -255,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)
				wait(0.4)
				ListMessage.Messages.BackgroundTimer.BackgroundLoad:TweenSize(
					UDim2.new(0, 0,0, 62),
					"Out",
					"Quad",
					0.4,
					false
				)

				ListMessage.Messages.BackgroundTimer.BackgroundTransparency=1
				ListMessage.Messages.BackgroundTimer.Others.Images.ImageLabel.ImageTransparency=1
				ListMessage.Messages.BackgroundTimer.Others.Texts.InfoText.TextTransparency=1
				wait(0.7)
				game.Lighting.CelestialBlur:destroy()
				TestGui.MainBackgroundFrame.Parent:Destroy()
			else
				game:GetService("TweenService"):Create(TestGui.Load.Start.LoadingScreen.SpinningCircle,TweenInfo.new(1.2,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
				game:GetService("TweenService"):Create(game.Lighting.CelestialBlur,TweenInfo.new(0.10,Enum.EasingStyle.Linear),{Size = 0}):Play()
				wait(2)
				LoginGui.Visible = true
				game:GetService("TweenService"):Create(LogInGuiTransparencyValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
				game:GetService("TweenService"):Create(LogInGuiTransparencyTextValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
				game:GetService("TweenService"):Create(LogInGuiTransparencyBarValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.6}):Play()
				if game:GetService("UserInputService").TouchEnabled == true then
					Sound3:Play()
					TextNotify="Mobile mode enabled"
					ImageID="rbxassetid://12988744896"
					NotifyColor=Color3.new(1, 0.847059, 0.384314)
					TestGui.Instances.Messages:Clone().Parent = ListMessage
				end
				wait(2)
				TestGui.Load.Start:Destroy()
			end
		end
		--game:GetService("TweenService"):Create(AsuraX.Load.Start.LoadingScreen.SpinningCircle,TweenInfo.new(1.2,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
		game:GetService("TweenService"):Create(game.Lighting.CelestialBlur,TweenInfo.new(0.10,Enum.EasingStyle.Linear),{Size = 0}):Play()
		wait(2)
		LoginGui.Visible = true
		game:GetService("TweenService"):Create(LogInGuiTransparencyValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(LogInGuiTransparencyTextValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
		game:GetService("TweenService"):Create(LogInGuiTransparencyBarValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.6}):Play()
		game.Lighting.CelestialBlur:Destroy()
		TestGui.Load.Start:Destroy()
		--versionchecker
		if Instances.Version.Value=="1.08" then
			warn("Checkpassed")
		end
	end
else
	wait(0.5)
	LoadSound:Play()
	LoginGui.Visible = true
	game:GetService("TweenService"):Create(game.Lighting.CelestialBlur,TweenInfo.new(0.10,Enum.EasingStyle.Linear),{Size = 0}):Play()
	game:GetService("TweenService"):Create(AsuraX.Load.Start.LoadingScreen.SpinningCircle,TweenInfo.new(1.2,Enum.EasingStyle.Linear),{ImageTransparency = 1}):Play()
	game:GetService("TweenService"):Create(LogInGuiTransparencyValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
	game:GetService("TweenService"):Create(LogInGuiTransparencyTextValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play()
	game:GetService("TweenService"):Create(LogInGuiTransparencyBarValue2.TextLabel,TweenInfo.new(0.1,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.6}):Play()
	game.Lighting.CelestialBlur:Destroy()
	AsuraX.Load.Start:Destroy()
end
