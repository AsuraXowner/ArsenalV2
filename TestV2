MainGui.DownGui.ButtonsListOfSettings.Button1Settings4.Text1.MouseButton1Click:Connect(function()
	MainGui.DownGui.ButtonsListOfSettings.Button1Settings4.Text2.Text = "Selected: ".."?"
	local input = userInputService.InputBegan:Wait()
	local keyCode = input.KeyCode
	if keyCode ~= Enum.KeyCode.Unknown and keyCode ~= Enum.KeyCode.Backspace then
		if userInputService:GetFocusedTextBox() ~= nil then return end
		Configs.ESP.KeybindActivate = keyCode
		MainGui.DownGui.ButtonsListOfSettings.Button1Settings4.Text2.Text = "Selected: "..Configs.ESP.KeybindActivate.Name
	else
		MainGui.DownGui.ButtonsListOfSettings.Button1Settings4.Text2.Text = "Selected: "..Configs.ESP.KeybindActivate.Name
	end
end)

MainGui.DownGui.ButtonsListOfSettings.Button2Settings1.Text1.MouseButton1Click:Connect(function()
