MainGui.MainMiddleButtons.Section1Gui.Button1Background.Text1.MouseButton1Click:Connect(function()
	if Button1SettingsValue==false then
		Button1SettingsValue=true
		MainGui.DownGui.ButtonsListOfSettings.Button1Settings.Visible = true
		MainGui.DownGui.ButtonsListOfSettings.Button1Settings1.Visible = true
		MainGui.DownGui.ButtonsListOfSettings.Button1Settings2.Visible = true
		MainGui.DownGui.ButtonsListOfSettings.Button1Settings3.Visible = true
		MainGui.DownGui.ButtonsListOfSettings.Button1Settings4.Visible = true
		MainGui.DownGui.ButtonsListOfSettings.Button1Settings5.Visible = true
		MainGui.DownGui.ButtonsListOfSettings.Button2Settings1.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button3Settings1.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button3Settings2.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button3Settings3.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button3Settings4.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button4Settings1.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button4Settings2.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button4Settings3.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button4Settings4.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button5Settings1.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button6Settings1.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Button7Settings1.Visible = false
		MainGui.DownGui.ButtonsListOfSettings.Visible=true
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings2Value.ButtonsSettings2TextLabel,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play() 
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings2TextValue.ButtonsSettings2TextLabel1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings3Value.ButtonsSettings3TextLabel,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play() 
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings3TextValue.ButtonsSettings3TextLabel1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings4Value.ButtonsSettings4TextLabel,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play() 
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings4TextValue.ButtonsSettings4TextLabel1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		wait(0.1)
		MainGui.DownGui.ButtonsListOfSettings:TweenPosition(--Timer
			UDim2.new(0.138, 0,-3.36, 0),
			"Out",
			"Quad",
			0.2,
			false
		)
		MainGui.DownGui.ButtonsListOfSettings:TweenSize(--Timer
			UDim2.new(0, 604,0, 118),
			"Out",
			"Quad",
			0.2,
			false
		)
		wait(0.4)
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings1Value.ButtonsSettings1TextLabel,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 0}):Play() 
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings1TextValue.ButtonsSettings1TextLabel1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 0.8}):Play()
	else
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings1Value.ButtonsSettings1TextLabel,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play() 
		game:GetService("TweenService"):Create(TestGui.MainBackgroundFrame.Instances.ButtonsSettings1TextValue.ButtonsSettings1TextLabel1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundTransparency = 1}):Play()
		wait(0.5)
		MainGui.DownGui.ButtonsListOfSettings:TweenSize(--Timer
			UDim2.new(0, 604,-0, 0),--{0.138, 0},{-0.225, 0}
			"Out",
			"Quad",
			0.2,
			false
		)
		MainGui.DownGui.ButtonsListOfSettings:TweenPosition(--Timer
			UDim2.new(0.138, 0,-0.225, 0),
			"Out",
			"Quad",
			0.2,
			false
		)
		wait(0.2)
		MainGui.DownGui.ButtonsListOfSettings.Visible=false
		Button1SettingsValue=false
