MainGui.MainMiddleButtons.Section2Gui.Button6Background.Button1.ImageButton2.MouseButton1Click:Connect(function()
	MainGui.MainMiddleButtons.Section2Gui.Button6Background.Button1.ImageButton1:TweenPosition(--Timer
		UDim2.new(0, 2,0.074, 0),
		"Out",
		"Quad",
		0.2,
		false
	)
	game:GetService("TweenService"):Create(MainGui.MainMiddleButtons.Section2Gui.Button6Background.Button1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(126, 154, 255)}):Play()
	MainGui.MainMiddleButtons.Section2Gui.Button6Background.Button1.ImageButton2:TweenPosition(--Timer
		UDim2.new(0, 2,0.889, 0),
		"Out",
		"Quad",
		0.2,
		false
	)
	game:GetService("TweenService"):Create(MainGui.MainMiddleButtons.Section2Gui.Button6Background.Button1.ImageButton1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(0, 0, 0)}):Play()
	Configs.ESP.Enabled=true
	--ESP Velocity

	function getRoot(char)
		local rootPart = char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
		return rootPart
	end


	function round(num, numDecimalPlaces)
		local mult = 10^(numDecimalPlaces or 0)
		return math.floor(num * mult + 0.5) / mult
	end


local function ESP(plr)
		task.spawn(function()
			wait(0.01)
			for i,v in pairs(game:GetService("Workspace"):GetChildren()) do
				if v.Name == plr.Name..'_ESP' then
					v:Destroy()
				end
			end
			task.wait(0.01)
			if Configs.ESP.Enabled==true then
				if plr.Character and plr.Name ~= game:GetService("Players").LocalPlayer.Name and not game:GetService("ReplicatedStorage"):FindFirstChild(plr.Name..'_ESP') then
					local ESPholder = Instance.new("Folder")
					ESPholder.Name = plr.Name..'_ESP'
					ESPholder.Parent = game:GetService("Workspace")
					repeat task.wait(1) until plr.Character and getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid")
					for b,n in pairs (plr.Character:GetChildren()) do
						if (n:IsA("BasePart")) then
							local a = Instance.new("BoxHandleAdornment")
							a.Name = plr.Name
							a.Parent = ESPholder
							a.Adornee = n
							a.AlwaysOnTop = true
							a.ZIndex = 10
							a.Size = n.Size
							a.Transparency = 0.3
							a.Color = plr.TeamColor
						end
					end
					if plr.Character and plr.Character:FindFirstChild('Head') then
						local BillboardGui = Instance.new("BillboardGui")
						local TextLabel = Instance.new("TextLabel")
						BillboardGui.Adornee = plr.Character.Head
						BillboardGui.Name = plr.Name
						BillboardGui.Parent = ESPholder
						BillboardGui.Size = UDim2.new(0, 100, 0, 150)
						BillboardGui.StudsOffset = Vector3.new(0, 1, 0)
						BillboardGui.AlwaysOnTop = true
						TextLabel.Parent = BillboardGui
						TextLabel.BackgroundTransparency = EspTextLabelBackground
						TextLabel.Position = UDim2.new(0, 0, 0, -50)
						TextLabel.Size = UDim2.new(0, 100, 0, 100)
						TextLabel.Font = EspTextLabelFont
						TextLabel.TextSize = EspTextLabelSize
						TextLabel.TextColor3 = EspTextLabelColor3
						TextLabel.TextStrokeTransparency = EspTextLabelStrokeTransparency
						TextLabel.TextYAlignment = Enum.TextYAlignment.Bottom
						TextLabel.Text = 'Name: '..plr.Name
						TextLabel.ZIndex = 10
						local espLoopFunc
						local teamChange
						local addedFunc
						addedFunc = plr.CharacterAdded:Connect(function()
							espLoopFunc:Disconnect()
							teamChange:Disconnect()
							ESPholder:Destroy()
							repeat task.wait(1) until getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid")
							ESP(plr)
							addedFunc:Disconnect()
							teamChange:Disconnect()
						end)
						teamChange = plr:GetPropertyChangedSignal("TeamColor"):Connect(function()
							espLoopFunc:Disconnect()
							addedFunc:Disconnect()
							ESPholder:Destroy()
							repeat task.wait(1) until getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid")
							ESP(plr)
							teamChange:Disconnect()
						end)
						local function espLoop()
							if game:GetService("Workspace"):FindFirstChild(plr.Name..'_ESP') then
								if plr.Character and getRoot(plr.Character) and plr.Character:FindFirstChildOfClass("Humanoid") and game:GetService("Players").LocalPlayer.Character and getRoot(game:GetService("Players").LocalPlayer.Character) and game:GetService("Players").LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
									local pos = math.floor((getRoot(game:GetService("Players").LocalPlayer.Character).Position - getRoot(plr.Character).Position).magnitude)
									TextLabel.Text = 'Name: '..plr.Name..' | Health: '..round(plr.Character:FindFirstChildOfClass('Humanoid').Health, 1)..' | Studs: '..pos
								end
							else
								teamChange:Disconnect()
								addedFunc:Disconnect()
								espLoopFunc:Disconnect()
							end
						end
						espLoopFunc = game:GetService("RunService").RenderStepped:Connect(espLoop)
					end
				else

				end
			end
		end)
	end

	for i,plr in game:GetService("Players"):GetPlayers() do
		ESP(plr)
	end

	game:GetService("Players").PlayerAdded:Connect(function(plr)
		ESP(plr)
	end)
end)
