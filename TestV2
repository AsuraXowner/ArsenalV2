	MainGui.MainMiddleButtons.Section2Gui.Button4Background.Button1.ImageButton2:TweenPosition(--Timer
		UDim2.new(0, 2,0.074, 0),
		"Out",
		"Quad",
		0.2,
		false
	)
	game:GetService("TweenService"):Create(MainGui.MainMiddleButtons.Section2Gui.Button4Background.Button1.ImageButton2,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(126, 112, 167)}):Play()
	Configs.Lighting.FullBright=false
	L.GlobalShadows = true
	L.ShadowSoftness = 0.2
	L.Brightness = 2
end)
MainGui.MainMiddleButtons.Section2Gui.Button4Background.Button1.ImageButton2.MouseButton1Click:Connect(function()
	MainGui.MainMiddleButtons.Section2Gui.Button4Background.Button1.ImageButton1:TweenPosition(--Timer
		UDim2.new(0, 2,0.074, 0),
		"Out",
		"Quad",
		0.2,
		false
	)
	game:GetService("TweenService"):Create(MainGui.MainMiddleButtons.Section2Gui.Button4Background.Button1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{BackgroundColor3=Color3.fromRGB(126, 154, 255)}):Play()
	MainGui.MainMiddleButtons.Section2Gui.Button4Background.Button1.ImageButton2:TweenPosition(--Timer
		UDim2.new(0, 2,0.889, 0),
		"Out",
		"Quad",
		0.2,
		false
	)
	game:GetService("TweenService"):Create(MainGui.MainMiddleButtons.Section2Gui.Button4Background.Button1.ImageButton1,TweenInfo.new(0.2,Enum.EasingStyle.Linear),{ImageColor3=Color3.fromRGB(0, 0, 0)}):Play()
	Configs.Lighting.FullBright=true
	if L:FindFirstChildOfClass("Atmosphere") then
		L:FindFirstChildOfClass("Atmosphere").Density = 0
		L:FindFirstChildOfClass("Atmosphere").Offset = 0
	else
		L.GlobalShadows = false
		L.ShadowSoftness = 0
		L.Brightness = 4
		--warn("no atmosphere")
	end
end)
